{
	"extends":"eslint:recommended",
	"parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
	"parserOptions": {
   		"ecmaVersion": 8,
   		"globalReturn": false
	},
	"rules": {
        "curly": [2, "multi-line"],
        "no-shadow": 0,
        "no-underscore-dangle": 0,
        "no-unused-expressions": 0,
        "no-unused-vars": 1,
        "quotes": [1, "single", "avoid-escape"],
        "semi": 1,
        "strict": 0,
        "no-console": 0
    }
}